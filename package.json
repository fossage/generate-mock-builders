{
  "name": "generate-mock-builders",
  "version": "0.1.0",
  "description": "A tool to generate mock data builders from a single set of seed data",
  "main": "./cli.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fossage/mock-generator.git"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:eslint": "eslint --fix --project .",
    "test": "run-s build test:*",
    "watch": "run-p \"build:main -- -w\"",
    "clean": "trash build test",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('mock-generator must be installed with Yarn: https://yarnpkg.com/')\""
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "pluralize": "^8.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@types/request-promise": "^4.1.44",
    "@types/lodash": "^4.14.144",
    "@types/node-fetch": "^2.5.2",
    "@types/pluralize": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-eslint": "^10.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "trash-cli": "^3.0.0",
    "typescript": "^3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/fossage/mock-generator/issues"
  },
  "homepage": "https://github.com/fossage/mock-generator#readme",
  "author": "Justin Foss <jfoss124@gmail.com>"
}
